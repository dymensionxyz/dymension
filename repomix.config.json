{
  "output": {
    "filePath": ".ai/repo.txt",
    "removeComments": false,
    "removeEmptyLines": false,
    "showLineNumbers": false,
    "includeEmptyDirectories": false,
    "instructionFilePath": ".ai/prompt.md"
  },
  "include": ["**/*.go", "**/*.proto", ".ai/**/*.md"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": ["**/*.pb.go", "**/*_test.go", "**/*.pb.gw.go", "testutil/", ".ai/prompt.md"]
  },
  "security": {
    "enableSecurityCheck": true
  }
}