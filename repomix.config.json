{
  "output": {
    "filePath": ".ai/repo.txt",
    "removeComments": false,
    "removeEmptyLines": false,
    "showLineNumbers": false,
    "includeEmptyDirectories": false,
    "instructionFilePath": ".ai/prompt.md",
    "style": "plain"
  },
  "include": ["**/x/rollapp/**/*.go"],
  "ignore": {
    "useGitignore": false,
    "useDefaultPatterns": true,
    "customPatterns": ["**/*.pb.go", "**/*_test.go", "**/*.pb.gw.go", "testutil/", ".ai/prompt.md", "**/cli/**"]
  },
  "security": {
    "enableSecurityCheck": true
  }
}